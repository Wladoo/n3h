{
  "name": "n3h-monorepo-manager",
  "version": "0.0.1",
  "description": "this top-level is just tools for helping manage the monorepo",
  "main": "n3h-monorepo-manager/lib/index.js",
  "scripts": {
    "this-lint": "standard --fix 'n3h-monorepo-manager/lib/**/*.js' 'n3h-monorepo-manager/bin/**/*.js'",
    "new": "node n3h-monorepo-manager/bin/monoman.js new",
    "install-all": "node n3h-monorepo-manager/bin/monoman.js install",
    "test-all": "node n3h-monorepo-manager/bin/monoman.js test",
    "docs-all": "node n3h-monorepo-manager/bin/monoman.js docs",
    "test": "npm run this-lint && npm run test-all && npm run docs-all"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "standard": "^11.0.1"
  },
  "dependencies": {
    "handlebars": "^4.0.12",
    "jsdoc-to-markdown": "^4.0.1",
    "prompt": "^1.0.0"
  },
  "projects": [
    "n3h-common",
    "mosodium",
    "mosocket",
    "hashcache",
    "ipc",
    "n3h",
    "hc-p2p-libp2p",
    "test-suite-executor",
    "pitr"
  ]
}
